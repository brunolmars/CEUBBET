<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ceubetjava">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CeubetJava"
        tools:targetApi="31">
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameSelectionActivity"
            android:exported="false"
            android:theme="@style/Theme.CeubetJava" />
        <activity
            android:name=".slot77.view.Slot77MainActivity"
            android:exported="false"
            android:theme="@style/Theme.CeubetJava" />
        <activity
            android:name=".batalhanaval.BatalhaNavalActivity"
            android:exported="false"
            android:theme="@style/Theme.CeubetJava" />
        <activity
            android:name=".blackjack.view.BlackjackMainActivity"
            android:exported="false"
            android:theme="@style/Theme.CeubetJava" />
        <activity
            android:name=".blackjack.view.GameActivity"
            android:exported="false"
            android:theme="@style/Theme.CeubetJava" />
        <activity
            android:name=".blackjack.view.RulesActivity"
            android:exported="false"
            android:theme="@style/Theme.CeubetJava" />
    </application>

</manifest>